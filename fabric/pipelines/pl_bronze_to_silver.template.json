{
  "$schema": "https://fabric.microsoft.com/schemas/pipeline/2024-01-01-preview",
  "name": "pl_bronze_to_silver_streaming",
  "description": "Runs the Bronze â†’ Silver notebook on a schedule",
  "activities": [
    {
      "name": "BronzeToSilverNotebook",
      "type": "Notebook",
      "properties": {
        "notebookResourceId": "<fabric-notebook-resource-id: bronze_to_silver>",
        "parameters": {},
        "timeout": "00:30:00"
      }
    }
  ],
  "triggers": [
    {
      "name": "Every5Minutes",
      "type": "Schedule",
      "properties": {
        "recurrence": "PT5M",
        "startTime": "<utc-iso8601>"
      }
    }
  ]
}

